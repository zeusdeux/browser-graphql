{
  "name": "browser-graphql",
  "version": 2,
  "scope": "zdx",
  "alias": ["browser-graphql.zdx.cat"],
  "public": true,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "demo/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/", "methods": ["GET"], "dest": "/demo/index.html" },
    { "src": "/(.*)", "methods": ["GET"], "dest": "/demo/$1" }
  ],
  "github": {
    "enabled": true
  }
}
