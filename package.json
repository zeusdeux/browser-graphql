{
  "name": "@zeusdeux/serverless-graphql",
  "version": "0.0.1",
  "description": "Run GraphQL from anywhere without the need for server",
  "main": "dist/serverless-gql.cjs.js",
  "module": "dist/serverless-gql.esm.js",
  "unpkg": "umd/serverless-gql.umd.js",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist && rimraf umd",
    "bundle": "rollup -c ./rollup.config.js",
    "build": "npm run clean && npm run bundle",
    "prepack": "npm run build",
    "watch": "rollup -c ./rollup.config.js -w"
  },
  "keywords": [
    "graphql",
    "browser",
    "serverless",
    "no server"
  ],
  "author": "Mudit Ameta <zeusdeux@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "np": "^5.2.1",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-terser": "^5.1.3"
  },
  "peerDependencies": {
    "graphql": "^14.5.8"
  },
  "engines": {
    "node": "12.x"
  }
}
